block variables
  //- 変数を管理 _variables.pugを参照
  include _variables
  //- mixinを読み込む
  include _mixin

doctype html
html(lang="ja")
  head
    include _meta

  body.l-body
    include /_include/_script_top
    include /_include/_header
    include /_include/_nav
    block content
    include /_include/_footer
    include /_include/_script_bottom
